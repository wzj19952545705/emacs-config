;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(yes-or-no-p-history extended-command-history file-name-history))
(setq yes-or-no-p-history '("yes"))
(setq extended-command-history '("eval-buffer"))
(setq file-name-history '("~/.emacs.d/init.el"))
